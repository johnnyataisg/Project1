
@{
    ViewBag.Title = "MissionInformation";
}

<ul class="breadcrumb">
    <li><a href=@Url.Action("Index", "Home")>Home</a></li>
    <li><a href=@Url.Action("Mission", "Home")>Mission Selection</a></li>
    <li>@ViewBag.Mission</li>
</ul>
<h2 class="mission-textbox">Mission Information</h2>
@Html.Raw((String)ViewBag.Output)
<div class="mission-textbox mission-question">
    <h4>Post any questions you have:</h4>
    <textarea class="form-control" rows="10" cols="75"></textarea>
</div>